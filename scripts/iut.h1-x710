export TE_PCI_INSTANCE_IUT_TST1="${TE_PCI_INSTANCE_H1_H2_X710}"
export TE_PCI_INSTANCE_IUT_TST1a="${TE_PCI_INSTANCE2_H1_H2_X710}"

export TE_PCI_VENDOR_TST1_IUT="${TE_PCI_VENDOR_H2_H1_X710}"
export TE_PCI_DEVICE_TST1_IUT="${TE_PCI_DEVICE_H2_H1_X710}"
export TE_PCI_INSTANCE_TST1_IUT="${TE_PCI_INSTANCE_H2_H1_X710}"
