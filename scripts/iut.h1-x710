export TE_ENV_IUT_NET_DRIVER="${TE_ENV_H1_NET_DRIVER_X710}"
export TE_ENV_TST1_NET_DRIVER="${TE_ENV_H2_NET_DRIVER_X710}"
test -z "${TE_ENV_IUT_DPDK_DRIVER}" -a -n "${TE_ENV_H1_DPDK_DRIVER_X710}" \
    && export TE_ENV_IUT_DPDK_DRIVER="${TE_ENV_H1_DPDK_DRIVER_X710}"
test -z "${TE_ENV_TST_DPDK_DRIVER}" -a -n "${TE_ENV_H2_DPDK_DRIVER_X710}" \
    && export TE_ENV_TST_DPDK_DRIVER="${TE_ENV_H2_DPDK_DRIVER_X710}"
